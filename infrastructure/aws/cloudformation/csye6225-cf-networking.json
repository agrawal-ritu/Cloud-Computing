{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "VPC": {
            "Type": "AWS::EC2::VPC",
            "Properties": {
                "CidrBlock": "191.0.0.0/16",
                "EnableDnsSupport": "true",
                "EnableDnsHostnames": "true",
                "InstanceTenancy": "default",
                "Tags":[{"Key" : "Name", "Value": { "Ref": "VPCNAME" }}]
            }
        },
        "PublicSubnet1": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "CidrBlock": "191.0.0.0/24",
                "AvailabilityZone": "us-east-1a",
		"MapPublicIpOnLaunch" : "true",
                "Tags": [
                    {
                        "Key": "foo",
                        "Value": "bar"
                    }
                ],
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "PublicSubnet2": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "CidrBlock": "191.0.1.0/24",
                "AvailabilityZone": "us-east-1b",
"MapPublicIpOnLaunch" : "true",
                "Tags": [
                    {
                        "Key": "foo",
                        "Value": "bar"
                    }
                ],
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "PublicSubnet3": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "CidrBlock": "191.0.2.0/24",
                "AvailabilityZone": "us-east-1c",
"MapPublicIpOnLaunch" : "true",
                "Tags": [
                    {
                        "Key": "foo",
                        "Value": "bar"
                    }
                ],
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "InternetGateway": {
            "Type": "AWS::EC2::InternetGateway",
            "Properties":{
			"Tags":[{"Key" : "Name", "Value": { "Ref": "IGWNAME" }}]
				}
        },
        "VPCGatewayAttachment": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "InternetGatewayId": {
                    "Ref": "InternetGateway"
                }
            }
        },
        "PublicRouteTable": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
		"Tags":[{"Key" : "Name", "Value": { "Ref": "PUBLICROUTETABLENAME" }}],
                "VpcId": {
                    "Ref": "VPC"
                }
            }
        },
        "EC2SRTA3HWRJ": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "PublicSubnet1"
                },
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                }
            }
        },
        "EC2SRTA3N4QG": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                },
                "SubnetId": {
                    "Ref": "PublicSubnet2"
                }
            }
        },
        "EC2SRTAQB28": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                },
                "SubnetId": {
                    "Ref": "PublicSubnet3"
                }
            }
        },
        "PublicInternetRoute": {
            "Type": "AWS::EC2::Route",
            "Properties": {
                "DestinationCidrBlock": "0.0.0.0/0",
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                },
                "GatewayId": {
                    "Ref": "InternetGateway"
                }
            }
        }
    },
 "Parameters": {

        "VPCNAME": {

          "Description" : "The stack name",

          "Type": "String"

        },

        "IGWNAME": {

          "Description" : "The Internet Gateway name",

          "Type": "String"

        },

        "PUBLICROUTETABLENAME": {

          "Description" : "The Public Route Table name",

          "Type": "String"

        }

      }
}
